/**index.wxss**/
page {
  background-color: #f7f7f7;
}
.tabbar {
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  height: 120rpx;
  display: flex;
  justify-content: space-around;
  z-index: 1;
  background-color: #fff;
}

.tabbar-item {
  margin: 25rpx 50rpx;
  text-align: center;
  font-size: 26rpx;
}
.tabbar-item image {
  height: 50rpx;
  width: 50rpx;
}

